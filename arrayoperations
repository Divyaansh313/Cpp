//A program to insert delete and search elements in an array using functions

#include <iostream>
using namespace std;

int search(int arr[], int n)
{
    int i;
    for (i = 0; i < n; i++)
    {
        cout<<arr[i]<<endl;
    }

}

int insert(int arr[], int n, int position, int in_elmt)
{
    int a;
    for(a=n; a>=position-1; a--)
    {
        arr[a+1]=arr[a];
    }
    arr[position-1]=in_elmt;
}

