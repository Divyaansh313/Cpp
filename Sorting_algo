//Sorting Algorithm
#include <iostream>
using namespace std;

int main()
{
	int a[999],n,i,j,temp;
	cout<<"enter the size of the array ";
	cin>>n;
	for(i=0;i<n;i++)
	{
	cout<<"enter the element "<<i+1<<": ";
	cin>>a[i];
	}
		for(i=0;i<n-1;i++)
	{
		for(j=i+1;j<n;j++)
		{
			if (a[i]>a[j])
			{
			temp=a[i];
			a[i]=a[j];
			a[j]=temp;
			}
		}
	}
