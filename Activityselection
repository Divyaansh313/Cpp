//Program to select activities

#include <iostream>
using namespace std;

void printMaxActivities(int s[], int f[], int n)
{
    int i, j;

    cout<<"Following activities are selected n "<<endl;


    i = 0;
    cout<<i<<endl;

    for (j = 1; j < n; j++)
    {

        if (s[j] >= f[i])
        {
            cout<<j<<endl;
            i = j;
        }
    }
}
